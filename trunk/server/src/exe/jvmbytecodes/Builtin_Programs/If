public class If
{
	public static void main(String[] args)
	{
		int num = 0;
		if(num == 0)
			num += 2;
		else
			num -=2;
		if(num > 0)
			num -= 5;
		else
			num += 2;
		if(num < 0)
			num += 3;
		else
			;
		if(num != 0)
			num += 2;
		else
			num += 5;
		if(num >= 0)
			num -= 5;
		else
			num += 5;
		if(num <= 0)
			num += 10;
		else
			num -= 5;
	}
}
